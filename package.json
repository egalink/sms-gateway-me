    "homepage": "https://github.com/egalink/sms-gateway.js#readme",
    "license": "MIT",
    "readmeFilename": "README.md",
    "main": "lib/index.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/egalink/sms-gateway.js.git"
    },
